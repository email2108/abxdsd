{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch GLXD Custom Dev Server",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "dev"],
      "cwd": "${workspaceFolder}/GLXD",
      "env": {
        "NODE_ENV": "development"
      },
      "console": "integratedTerminal"
    },
    {
      "name": "Launch LST Next.js Dev Server",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/lst/node_modules/.bin/next",
      "args": ["dev"],
      "cwd": "${workspaceFolder}/lst",
      "env": {
        "NODE_ENV": "development"
      },
      "console": "integratedTerminal"
    }
  ]
}
